<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/colorPrimaryDark"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.mars.httpapp.GroupDetailActivity"
    tools:showIn="@layout/activity_group_detail">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Squad Details"
        android:id="@+id/textView"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="54dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Class:"
        android:textColor="@color/white"
        android:id="@+id/textView2"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="68dp"
        android:layout_marginStart="68dp"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Class Number:"
        android:textColor="@color/white"
        android:id="@+id/textView3"
        android:layout_marginTop="36dp"
        android:phoneNumber="false"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/classNumberTextView"
        android:layout_toStartOf="@+id/classNumberTextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/classNumberTextView"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_marginLeft="62dp"
        android:layout_marginStart="62dp"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/classTextView"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignLeft="@+id/classNumberTextView"
        android:layout_alignStart="@+id/classNumberTextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Date:"
        android:textColor="@color/white"
        android:id="@+id/textView6"
        android:layout_marginTop="41dp"
        android:phoneNumber="false"
        android:layout_below="@+id/textView3"
        android:layout_alignRight="@+id/textView3"
        android:layout_alignEnd="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/dateTextView"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignLeft="@+id/classNumberTextView"
        android:layout_alignStart="@+id/classNumberTextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Time:"
        android:textColor="@color/white"
        android:id="@+id/textView8"
        android:layout_marginTop="39dp"
        android:phoneNumber="false"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Description:"
        android:textColor="@color/white"
        android:id="@+id/textView9"
        android:layout_marginTop="37dp"
        android:phoneNumber="false"
        android:layout_below="@+id/textView8"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/timeTextView"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignLeft="@+id/dateTextView"
        android:layout_alignStart="@+id/dateTextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/descriptionTextView"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:layout_alignTop="@+id/textView9"
        android:layout_alignLeft="@+id/timeTextView"
        android:layout_alignStart="@+id/timeTextView" />

</RelativeLayout>
